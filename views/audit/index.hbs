<div class="table-responsive">
    <table class="table table-hover table-sm email-table">
        <thead>
            <tr>
                <th scope="col" class="email-check-row">
                    <input type="checkbox">
                </th>
                <th scope="col" class="email-from-cell">From</th>
                <th scope="col" class="email-from-cell">To</th>
                <th scope="col" class="email-subject-cell">Subject</th>
                <th scope="col" class="email-date-cell">Date</th>
            </tr>
        </thead>
        <tbody>

            <tr>
                <td class="email-check-row">
                    <input type="checkbox" class="checkMessage" data-message="{{id}}" />
                </td>

                <td class="email-table-cell email-from-cell">
                    <a href="{{url}}" class="email-list-a">
                        {{displayFrom}}
                    </a>
                </td>

                <td class="email-table-cell email-from-cell">
                    <a href="{{url}}" class="email-list-a">
                        {{displayTo}}
                    </a>
                </td>

                <td class="email-table-cell email-subject-cell">
                    <a href="{{url}}" class="email-list-a"> {{subject}} <span class="text-muted">{{#if subject}}–
                            {{/if}}{{intro}}</span> </a>
                </td>

                <td class="email-table-cell text-right email-date-cell">
                    <a href="{{url}}" class="email-list-a">
                        {{#if attachments}}
                        <span class="btn-inner--icon float-left">
                            <i data-feather="paperclip"></i>
                        </span>
                        {{/if}}
                        <span class="datestring-fixed" title="{{date}}"></span>
                    </a>
                </td>
            </tr>

        </tbody>
    </table>
</div>